# Часть 3
a = int(input())
b = int(input())
print(a + b)
print(a - b)
print(a * b)
print(a ** b)
print(a // b)

# Часть 4
x1 = ((17 / 2) * 3) + 2
x2 = (2+ ((17 / 2) * 3))
x3 = ((19 % 4) + ((15 / 2) * 3))
x4 = ((15 + 6) - (10 * 4))
x5 = (((17 / 2) % 2) * (3 ** 3))
print(x1)
print(x2)
print(x3)
print(x4)
print(x5)

# Часть 5
string = "abcdefghi"
print(string.capitalize())
print(string.upper())
print(string[:3])
print(string[1::2])
